// Car.java file
class Car {
	String color = "white";
	int seat = 4;
	int speed;
	int weel = 4;

	Car() {
		// TODO Auto-generated constructor stub
	}
	
	void paint(String car_color) {
		color = car_color;
		System.out.println("This car is: "+ color);
	}
	
	void speed(int car_speed) {
		speed = car_speed;
		System.out.println("Car speed is: "+ speed);
	}
	
}
