// Super class Car

public class Car{
    int wheels = 4;
    String color = "white";
    String type;
    int speed;
    int seats = 4;
    
    public String paint (string car_color, String car_name){
        color = car_color;
        return color;
    }
    
    pulic int speed(int car_speed){
        speed = car_speed;
        return speed;
    }
}
